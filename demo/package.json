{
    "name": "awsm-renderer",
    "version": "0.1.0",
    "license": "(MIT OR Apache-2.0)",
    "scripts": {
        "serve:dev": "npm-run-all --parallel localmedia:serve trunk:serve:dev",
        "localmedia:serve": "cd media && http-server -g -b --cors -p 9000",
        "trunk:serve:dev": "cross-env RUSTFLAGS=--cfg=web_sys_unstable_apis trunk serve --features dev",
        "trunk:serve:release": "cross-env RUSTFLAGS=--cfg=web_sys_unstable_apis trunk serve --release",
        "trunk:build": "cross-env RUSTFLAGS=--cfg=web_sys_unstable_apis trunk build --release",
        "github:deploy:patch": "node ./github-deploy-patch.js"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.13",
        "cross-env": "^7.0.3",
        "cssnano": "^5.1.15",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.21",
        "postcss-cli": "^10.1.0",
        "postcss-import": "^15.1.0",
        "postcss-reporter": "^7.0.5",
        "postcss-url": "^10.1.3",
        "tailwindcss": "^3.2.7"
    }
}
